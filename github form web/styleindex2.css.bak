.header{
min-height:100vh;
width:100%;
background-image:url("D:project/saravanan/img/bulls/back.jpg");
background-position:center;
background-size:cover;
position:relative;
}
.nav-link ul
{
	text-align:center;
}
.nav-link ul li{
display:inline-block;
padding:30px 25px;
}
.nav-link ul li a{
text-decoration:none;
color:#ca03fc;
font-size:bold;
text-transform:uppercase;
}
.nav-link ul li::after{
display:block;
content:'';
width:0%;
height:2px;
background-color:red;
margin:0 auto;
transition:0.5s;
}
.nav-link ul li:hover::after{
width:100%;
}
.head{
font-style:italic;
text-transform:uppercase;
color:#f723a9;
margin:60px 0 0 490px;
}
.para{
margin:200px 0 0 250px;
font-style:italic;
color:#5c23f7;
font-size:20px;
}
#button{
text-align:center;
margin:0 0 -50px 595px;
border:2px solid #ca03fc;
border-radius:20px;
cursor:pointer;
font-size:bold;
background:#23f0f7;
display:inline-block;
position:relative;
}
#button:hover{
transition:1s;
border:2px solid red;
background:#c2f723;
}
#button .view-button{
text-decoration:none;
padding:5px;
display:inline-block;
position:relative;
}
#oval{
border:2px dashed red;
border:10px;
}
